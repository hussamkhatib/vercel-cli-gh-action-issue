{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "vc-admin#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": ["NEXT_PUBLIC_ADMINAPP_URL", "NEXT_PUBLIC_WEBAPP_URL"]
    },
    "vc-web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": ["NEXT_PUBLIC_WEBAPP_URL", "VERCEL_URL", "NODE_ENV"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["//#env-check:common"],
      "outputs": [],
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "lint:report": {
      "cache": false,
      "outputs": ["lint-results"]
    },
    "//#env-check:common": {
      "cache": false,
      "inputs": ["./.env.example", "./.env"],
      "outputs": ["./.env"]
    },
    "//#test": {
      "cache": false,
      "outputs": []
    }
  },
  "globalDotEnv": [".env"],
  "globalEnv": ["DATABASE_URL"]
}
